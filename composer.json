{
    "name": "aloware/tenants-queue",
    "description": "Laravel package to provide fair consumption of jobs against multiple tenants.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sohrab Sheikhani",
            "email": "sohrab@aloware.com"
        },
        {
            "name": "Hamed Kamrava",
            "email": "hamed@aloware.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "laravel/framework": "^5.7|^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^5.7|^6.0|^7.0|^8.0|^9.0",
        "illuminate/queue": "^5.7|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Aloware\\TenantsQueue\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aloware\\TenantsQueue\\TenantsQueueServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
